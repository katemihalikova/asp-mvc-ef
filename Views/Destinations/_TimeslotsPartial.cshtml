@model IEnumerable<ASP1.Models.Timeslot>

@foreach (var item in Model)
{
    <div class="col-6 col-sm-12 col-md-6 col-lg-4 pb-4">
        <button type="button" class="btn btn-light" data-timeslot-id="@item.ID">
            @Html.DisplayFor(modelItem => item.DateFrom)
            -
            @Html.DisplayFor(modelItem => item.DateTo)
        </button>
    </div>
}

@if (Model.Count() == 0)
{
    <span class="text-danger">No free term found.</span>
}
